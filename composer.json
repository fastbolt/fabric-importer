{
  "name": "fastbolt/fabric-importer",
  "type": "library",
  "description": "A library for the import of database-data from Microsoft Fabric.",
  "keywords": [
    "fastbolt",
    "database",
    "fabric",
    "microsoft",
    "import"
  ],
  "homepage": "https://github.com/fastbolt/fabric-importer",
  "license": "MIT",
  "authors": [
    {
      "name": "Steffen Leu√üink",
      "email": "slk@fastbolt.com",
      "homepage": "https://github.com/StvL97",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Fastbolt\\FabricImporter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fastbolt\\FabricImporter\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "prefer-stable": true
}
