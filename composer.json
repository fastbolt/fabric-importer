{
  "name": "fastbolt/fabric-importer",
  "type": "library",
  "description": "A library for the import of database-data from Microsoft Fabric.",
  "keywords": [
    "fastbolt",
    "database",
    "fabric",
    "microsoft",
    "import"
  ],
  "homepage": "https://github.com/fastbolt/fabric-importer",
  "license": "MIT",
  "authors": [
    {
      "name": "Steffen LeuÃŸink",
      "email": "slk@fastbolt.com",
      "homepage": "https://github.com/StvL97",
      "role": "Developer"
    }
  ],
  "require": {
      "php": ">=8.1",
      "doctrine/persistence": "^2.4|^3.0",
      "symfony/http-foundation": "^6.4.29",
      "symfony/config": "^6.0",
      "symfony/filesystem": "^6.0",
      "symfony/framework-bundle": "^6.4"
  },
  "require-dev": {
      "phpstan/phpstan": "^1.4",
      "phpunit/phpunit": "^9.5",
      "psalm/plugin-phpunit": "^0.18.4",
      "psalm/plugin-symfony": "^v5.0",
      "slevomat/coding-standard": "^7.0",
      "spryker/code-sniffer": "^0.17.4",
      "squizlabs/php_codesniffer": "^3.6",
      "vimeo/psalm": "^5.6",
      "weirdan/doctrine-psalm-plugin": "^v2.8"
  },
  "autoload": {
    "psr-4": {
      "Fastbolt\\FabricImporter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fastbolt\\FabricImporter\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "prefer-stable": true
}
